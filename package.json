{
  "name": "ice-by-claude",
  "version": "1.0.0",
  "description": "claude caude keeps an ice cube alive ($ice) - A Solana token protocol with automated fee-based buyback and health maintenance",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "clean": "rm -rf dist",
    "simulate": "ts-node scripts/simulate.ts",
    "executor:dry-run": "ts-node scripts/executor-dry-run.ts",
    "health:check": "ts-node scripts/health-check.ts",
    "types": "tsc --noEmit"
  },
  "keywords": [
    "solana",
    "token",
    "meme",
    "ice",
    "buyback",
    "protocol"
  ],
  "author": "claude",
  "license": "MIT",
  "dependencies": {
    "@solana/web3.js": "^1.93.0",
    "@solana/spl-token": "^0.4.0",
    "@raydium-io/raydium-sdk": "^1.3.0",
    "pino": "^8.16.2",
    "dotenv": "^16.3.1",
    "bs58": "^6.0.0",
    "axios": "^1.6.2"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "@types/node": "^20.10.5",
    "@types/bs58": "^4.0.2",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "prettier": "^3.1.1"
  }
}
